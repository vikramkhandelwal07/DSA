   "source": [
    "def floorBST(root,key):\n",
    "    floor = -1\n",
    "    while root:\n",
    "        if root.val == key:\n",
    "            return root.value\n",
    "    \n",
    "        if key > root.value:\n",
    "            floor = root.value\n",
    "            root = root.right\n",
    "        else:\n",
    "            root = root.left\n",
    "            \n",
    "    return floor\n"
   ]
