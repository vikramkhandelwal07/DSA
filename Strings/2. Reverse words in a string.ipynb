    "def reverseWord(s):\n",
    "    arr = s.split()\n",
    "    arr.reverse()\n",
    "    return \" \".join(arr)\n",
    "\n",
    "\n",
    "s = \"sky is blue in color\"\n",
    "reverseWord(s)"
