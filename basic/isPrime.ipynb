   "source": [
    "import math\n",
    "\n",
    "n = int(input())  \n",
    "count = 0\n",
    "\n",
    "\n",
    "for i in range(1, int(math.sqrt(n)) + 1):\n",
    "    if n % i == 0:\n",
    "        # If i divides n, then i and n//i are both divisors\n",
    "        count += 1\n",
    "        if i != n // i:\n",
    "            count += 1\n",
    "\n",
    "if count == 2:\n",
    "    print(\"YES\")\n",
    "else:\n",
    "    print(\"NO\")\n"
   ]
