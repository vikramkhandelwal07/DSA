   "source": [
    "def search(root, key):\n",
    "  if not root:\n",
    "    return False\n",
    "  if root.value == key:\n",
    "    return True\n",
    "  if root.value > key:\n",
    "    return search(root.left,key)\n",
    "  else:\n",
    "    return search(root.right,key)  "
   ]
